//Number Variable
const a = 10;

module.exports = a;

// Multiple Variables
const a = 20;
const b = a;

module.exports = { a, b }

// Booleans
const a = false;
const b = true;

module.exports = { a, b }

//  Strings
const a = "World";
const b = "Hello World";
module.exports = { a,b }

// Mutable Let
let a = 3;

 a = 5;

module.exports = a;

// Comments
// all shoes are on sale this week!
const shoesCost = 49;

module.exports = shoesCost;

